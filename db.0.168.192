;
; BIND data file for local loopback interface
;
$ORIGIN 0.168.192.in-addr.arpa.
$TTL	604800
@	IN	SOA	pc1 root (
			      20180424		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
	IN	NS	pc1.practicadaw.net.
100	IN	PTR	pc1.practicadaw.net.
101	IN	PTR	pc2.practicadaw.net.
ftp	CNAME	pc2
250	IN	PTR	pc3.practicadaw.net.
correro	CNAME	pc3


